@model IEnumerable<GCD0705.AppDev.ViewModels.UsernameWithRoleViewModel>
@{
	ViewBag.Title = "Profiles";
}

<h2>Profiles</h2>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>User Name</th>
			<th>User Roles</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Username</td>
				<td>@item.Rolename</td>
			</tr>
		}
	</tbody>
</table>

